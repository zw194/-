{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["node"],
  "rules": {
    "prettier/prettier": "error",
    "no-useless-catch": 0,
    "no-unused-vars": 1,
    "no-var": 1
    // 在这里添加自定义的规则
  }
}
